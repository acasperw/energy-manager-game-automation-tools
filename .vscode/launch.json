{
  "version": "0.2.0",
  "configurations": [
    // Debug Scheduler (index.ts)
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Scheduler",
      "program": "${workspaceFolder}/src/index.ts",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "args": [],
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.ts"
      ],
    },
